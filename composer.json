{
    "name": "laravel-notification-channels/z-api",
    "description": "Z-API Notification Channel for Laravel.",
    "homepage": "https://github.com/laravel-notification-channels/z-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Saade",
            "email": "saade@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/notifications": "~10.0 || ~11.0",
        "illuminate/support": "~10.0 || ~11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ZApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ZApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
